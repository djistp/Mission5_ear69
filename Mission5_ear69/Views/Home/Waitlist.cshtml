@{

    ViewData["Titile"] = "Waitlist";
}
@model List<MovieResponse>

<table class="table table-bordered table-striped" >
    <thead>
        <tr>
            <td>Category</td>
            <td>Title</td>
            <td>Year</td>
            <td>Director</td>
            <td>Rating</td>
            <td>Edit</td>
            <td>Lent</td>
            <td>Notes</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
            {
        <tr>
            <td>@x.Category.CategoryName</td>
            <td>@x.Title</td>
            <td>@x.Year</td>
            <td>@x.Director</td>_ea
            <td>@x.Rating</td>
            <td>@x.Edit</td>
            <td>@x.Lent</td>
            <td>@x.Notes</td>
            <td><a asp-controller="Home" asp-action="Edit" asp-route-movieid="@x.MovieId">Edit</a></td>
            <td><a asp-controller="Home" asp-action="Delete" asp-route-movieid="@x.MovieId">Delete</a></td>

        </tr>
            }
    </tbody>
</table>
